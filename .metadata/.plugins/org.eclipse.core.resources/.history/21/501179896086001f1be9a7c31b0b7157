package lambdas;

//Thread Ã© um processo que pode ser trabalhado separadamente
public class Threads {
	public static void main(String[] args) {
		
		Runnable tarefa1 = new Trabalho1();
		Runnable tarefa2 = new Runnable() {
			
			public void run() {
				for (int i = 0; i < 100; i++) {
					System.out.println("Tarefa #01");
				}
			}
		};
		
		new Thread(trabalho1)
	}
}
