package streams;

public class Produto {
	final String nome;
	final String preco;
	final double desconto;
	final double valorFrete;

	public Produto(String nome, double d, double desconto, double valorFrete) {
		this.nome = nome;
		this.preco = d;
		this.desconto = desconto;
		this.valorFrete = valorFrete;
	}

}
