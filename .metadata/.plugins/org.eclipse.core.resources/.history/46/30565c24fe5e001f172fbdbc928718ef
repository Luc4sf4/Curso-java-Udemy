package classe;

public class Produto {
			
	//Atributo
	String nome;
	double preco;
	double desconto;
				 //Nome da classe
	Produto(String nomeInicial){
		nome = nomeInicial;
	}
	
	public Produto() {
		// TODO Auto-generated constructor stub
	}
	
	double precoComDesconto(){
		return preco * (1 - desconto);
	}

}
