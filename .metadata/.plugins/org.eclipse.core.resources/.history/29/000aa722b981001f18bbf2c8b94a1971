package oo.encapsulamento.casaB;

import oo.encapsulamento.casaA.Ana;

// o que pedro pode acessar de ana ? 
public class Pedro extends Ana {
	
	

	void testeAcessos() {
//		 segredo
//		 facoDentroDeCasa
//		formaDeFalar 
//		todosSabem 
		
		Ana mae = new Ana();
		
		//System.out.println(mae.segredo);// não posso acessar, é privado
		System.out.println(mae.facoDentroDeCasa);// visibilidade só dentro do pacote
		System.out.println(mae.formaDeFalar);//alem de poder  ser herdado, está dentro do mesmo pacote
		System.out.println(mae.todosSabem);// é publico
	}

}
