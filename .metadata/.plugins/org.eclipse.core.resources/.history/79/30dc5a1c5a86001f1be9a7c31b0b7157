package lambdas;

import java.util.function.Predicate;

//Predicado Ã© uma interface funcional que recebe parametro e retorna verdadeiro ou falso 
public class PredicadoComposicao {

	public static void main(String[] args) {
		
		
		//se o modulo do numero divido por 2 for 0 
		Predicate<Integer> isPar = num -> num % 2 ==0 ;
		
	}
}
