package oo.herenca;

public class Jogador {
	
	int x;
	int y;
	

	boolean andar(Direcao direcao){
		
		switch (direcao) {
		case value: {
			
			yield type;
		}
		default:
			throw new IllegalArgumentException("Unexpected value: " + key);
		}
		
		if(direcao == Direcao.NORTE){
			y++;
		}
		return true;
	}
}
