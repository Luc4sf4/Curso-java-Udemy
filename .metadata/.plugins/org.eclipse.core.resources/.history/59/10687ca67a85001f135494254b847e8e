package lambdas;

import java.util.function.BinaryOperator;

public class CalculoTeste2 {
	public static void main(String[] args) {
		
		// int 
		

		// função lambda
		BinaryOperator<Double> calc = (x, y) -> {
			return x + y;
		};
		System.out.println(calc.apply(2, 3));

		calc = (x, y) -> x * y;
		System.out.println(calc.apply(2, 3));
		
	
		
	
	}

}
