package classe;

public class Produto {

	// Atributo
	String nome;
	double preco;
	double desconto;

	// Construtor padrão que não receberá parametros
	Produto() {
	}

	

	// Construtor com dois parametros
	Produto(String nomeInicial, double precoInicial) {
		nome = nomeInicial;
		preco = precoInicial;
	}

	double precoComDesconto() {
		return preco * (1 - desconto);
	}

}
