package oo.herenca.desafio;

public class Ferrari extends Carro implements Esportivo {

	public Ferrari() {
		this(315);
	}

	public Ferrari(int velocidadeMaxima) {
		super(velocidadeMaxima);
		delta = 15;

	}

	@Override
	public void ligarTurbo() {
		delta = 35;
	}

	@Override
	public void desligarTurbp() {
		delta = 15;
	}

//	@Override
//	void acelerar(){
//		super.acelerar();
//	}

}
