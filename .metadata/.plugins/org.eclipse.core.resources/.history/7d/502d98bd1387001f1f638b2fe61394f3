package streams;

public class Produto {
	final String nome;
	final String preco;
	double desconto;
	double valorFrete;
	public Produto(String nome, String preco, double desconto, double valorFrete) {
		this.nome = nome;
		this.preco = preco;
		this.desconto = desconto;
		this.valorFrete = valorFrete;
	}
	
	

}
